{
  "name": "@karfau/xstate-plantuml",
  "version": "0.6.0-dev",
  "description": "Visualize a xstate or react-automata statechart as a plantuml state diagram",
  "keywords": [
    "xstate",
    "react-automata",
    "plantuml",
    "statecharts",
    "visualization"
  ],
  "main": "src/core.js",
  "types": "src/core.d.ts",
  "files": [
    "examples", "src"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/karfau/xstate-plantuml.git"
  },
  "scripts": {
    "test": "jest",
    "tsc": "tsc",
    "types": "rm -f src/*.d.ts && tsc --project tsconfig.types.json && git add src/*.d.ts",
    "plantuml": "docker-compose run --rm -T plantuml"
  },
  "devDependencies": {
    "@types/jest": "26.0.20",
    "@types/node": "14.14.22",
    "execa": "5.0.0",
    "jest": "26.6.3",
    "typescript": "4.1.3",
    "xstate": "4.15.4",
    "xstate3": "npm:xstate@3.3.3"
  },
  "peerDependencies": {
    "xstate": "^3.3.3 || ^4",
    "execa": ">=4"
  }
}
